{
  "name": "knut7/knut7",
  "type": "library",
  "description": "Min framwork em mvc para PHP 7.0.0 ou superior",
  "keywords": [
    "framwork",
    "mvc"
  ],
  "homepage": "https://github.com/Knut7/Knut7",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcio Zebede",
      "email": "marciozebedeu@gmail.com",
      "homepage": "http://artphoweb.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "knut7/framework": "*",
    "phpmailer/phpmailer": "~6.0",
    "phpunit/phpunit": "^7.5"


  },
  "require-dev": {
    "php": ">=7.0.0",
    "knut7/framework": "dev-master",
    "phpmailer/phpmailer": "~6.0",
    "phpunit/phpunit": "^7.5"


  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
    "minimum-stability": "dev",
    "prefer-stable": true

}
