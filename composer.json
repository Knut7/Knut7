{
  "name": "knut7/knut7",
  "type": "library",
  "description": "Min framwork em mvc para PHP 7.0.0 ou superior",
  "keywords": [
    "framwork",
    "mvc"
  ],
  "homepage": "https://github.com/Knut7/Knut7",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcio Zebede",
      "email": "marciozebedeu@gmail.com",
      "homepage": "http://artphoweb.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "knut7/framework": "*",
    "phpmailer/phpmailer": "~6.0",
    "bshaffer/oauth2-server-php": "dev-develop",
    "bshaffer/oauth2-server-httpfoundation-bridge": "v1.0",
    "http-interop/http-server-middleware": "^1.1.1",
    "symfony/http-foundation": "^4.3",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.6"

  },
  "require-dev": {
    "php": ">=7.0.0",
    "knut7/php-graphic": "dev-master",
    "phpmailer/phpmailer": "~6.0",
    "swiftmailer/swiftmailer": "^6.0"
  },
     "autoload": {
        "psr-4": {
            "Ballybran\\": "src/Ballybran/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ballybran\\Tests\\": "tests/"
        }
    }
}

